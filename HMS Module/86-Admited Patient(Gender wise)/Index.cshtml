                                <div class="col-sm-5">
                                    No. Of Patient
                                    <input type="text" id="txtNoOfPatient" value="0" class="form-control" />
                                </div>
                            </div>

                            <div class="form-group row" style="margin-bottom: 0px;">
                                <div class="col-sm-7">
                                    Upazila
                                    @*@Html.DropDownListFor(model => model.Pres_UpazilaID, new SelectList(""), "Select District", htmlAttributes: new { @id = "ddlUpazila", @class = "chosen-select form-control"})*@

                                    <select id="ddlUpazila" style="background:white;" class="chosen-select form-control" multiple>
                                    </select>

                                </div>

                                <div class="col-sm-5">
                                    Union
                                    @*@Html.DropDownListFor(model => model.PresUnionID, new SelectList(""), "Select District", htmlAttributes: new { @id = "ddlUnion", @class = "chosen-select form-control" })*@
                                    <select id="ddlUnion" style="background:white;" class="chosen-select form-control" multiple>
                                    </select>
                                </div>
                            </div>


                            @*<div class="form-group" id="divAreaWise" style="margin-bottom: 0px;">

                                    <div class="col-sm-6" style="margin-left:-14px;">

                                    </div>

                                    <div class="col-sm-4" style="margin-left:-14px;">

                                    </div>
                                </div>*@

                            <div class="form-group row" style="margin-bottom: 0px;">
                                <div class="col-sm-7">
                                    Token Type
                                    @Html.DropDownList("TokenType", new List<SelectListItem>
                                        {
                                            new SelectListItem {Text = "New", Value = "New"},
                                            new SelectListItem {Text = "Old", Value = "Old"},
                                        }, "Select Token Type", new { @class = "chosen-select form-control" })
                                </div>

                                <div class="col-sm-5">
                                    Free Ticket
                                    <input type="checkbox" class="ace-switch input-lg ace-switch-yesno bgc-purple-d1 text-grey-m2" id="chkStatus" onclick="UpdateStatus()" />
                                    @Html.HiddenFor(model => model.Status, new { @class = "form-control", @placeholder = "Status", @id = "txtStatus" })
                                </div>
                            </div>

                            <div class="form-group row" style="margin-bottom: 0px;">
                                <div class="col-sm-7">
                                    Department
                                    @Html.DropDownListFor(model => model.DeptID, null, "Select Department", htmlAttributes: new { @id = "ddlDept", @class = "chosen-select form-control" })
                                </div>
                                <div class="col-sm-5">
                                    Ward/Cabin
                                    @Html.DropDownListFor(model => model.WardCabinID, null, "Select Ward", htmlAttributes: new { @class = "chosen-select form-control", @id = "ddlWard" })
                                </div>
                            </div>

                            <div class="form-group" style="margin-bottom: 0px;">
                                Doctor
                                @Html.DropDownList("DOC_ID", new SelectList(" "), "Select Doctor", htmlAttributes: new { @id = "ddlDoc", @class = "chosen-select form-control" })
                            </div>

                            <div class="form-group" style="margin-bottom: 0px;">
                                Project
                                @Html.DropDownListFor(model => model.ProjectID, null, "Select Project", htmlAttributes: new { @id = "ddlProject", @class = "chosen-select form-control" })
                            </div>

                            <div class="form-group" style="margin-bottom: 0px;">
                                User
                                @Html.DropDownListFor(model => model.UserID, null, "Select User", htmlAttributes: new { @id = "ddlUser", @class = "chosen-select form-control" })
                            </div>

                            <div class="form-group" style="margin-bottom: 0px;">
                                Diagnosis
                                @Html.DropDownList("DiagnosisID", new SelectList(" "), "Select Diagnosis", htmlAttributes: new { @id = "ddlDiagnosis", @class = "chosen-select form-control" })
                            </div>

                            <div class="form-group" style="margin-bottom: 5px;">
                                Pharmacy Manufacturer
                                @Html.DropDownListFor(model => model.ManufacturerID, null, "Select Manufacturer", htmlAttributes: new { @id = "ddlManufacturerID", @class = "chosen-select form-control" })
                            </div>

                        </div>

                    </div>

                </div>

                <!-- /.card-body -->
            </div>
            <!-- /.card -->
        </div>
        <!-- /.container-fluid -->
    </section><!-- /.content -->
</div>

<script>

    $(document).ready(function () {

        var textbox = document.getElementById("txtStatus");
        textbox.value = "N";

        //load current date
        var today = new Date();
        var dd = String(today.getDate()).padStart(2, '0');
        var mm = String(today.getMonth() + 1).padStart(2, '0'); //January is 0!
        var yyyy = today.getFullYear();

        today = mm + '/' + dd + '/' + yyyy;
        $('#txtFromDate').val(today);
        $('#txtToDate').val(today);

        var TinyDatePicker = DateRangePicker.TinyDatePicker;
        TinyDatePicker('#txtFromDate', {
            mode: 'dp-below',
        })

        var TinyDatePicker = DateRangePicker.TinyDatePicker;
        TinyDatePicker('#txtToDate', {
            mode: 'dp-below',
        })

         if ('@TempData["Success"]' != "") {
             toastr.success('@TempData["Success"]', 'Success');
         }
         else if ('@TempData["Warning"]' != "") {
             toastr.warning('@TempData["Warning"]', 'Warning');
         }
            else if ('@TempData["Error"]' != "") {
             toastr.error('@TempData["Error"]', 'Error');
        }

        //clear all search criteria
        $("#txtFromDate").attr("disabled", false);
        $("#txtToDate").attr("disabled", false);
        $("#ddlOPServiceType").attr("disabled", true).trigger("chosen:updated");
        $("#TokenType").attr("disabled", true).trigger("chosen:updated");
        $("#ddlDept").attr("disabled", true).trigger("chosen:updated");
        $("#ddlDoc").attr("disabled", true).trigger("chosen:updated");
        $("#ddlProject").attr("disabled", true).trigger("chosen:updated");
        $("#ddlUser").attr("disabled", true).trigger("chosen:updated");
        $("#chkStatus").attr("disabled", true);
        $("#ddlDiagnosis").attr("disabled", true).trigger("chosen:updated");
        $("#AgeFrom").attr("disabled", true);
        $("#AgeTo").attr("disabled", true);
        $("#SummaryType").attr("disabled", true).trigger("chosen:updated");
        $("#ddlPT").attr("disabled", true).trigger("chosen:updated");
        $("#PatientType").attr("disabled", true).trigger("chosen:updated");
        $("#PatientID").attr("disabled", true);
        $("#PatientID").val('');
        $("#ddlServiceType").attr("disabled", true).trigger("chosen:updated");
        $("#ddlService").attr("disabled", true).trigger("chosen:updated");
        $("#ddlWard").attr("disabled", true).trigger("chosen:updated");
        $("#StartTime").attr("disabled", true);
        $("#EndTime").attr("disabled", true);
        $("#ddlManufacturerID").attr("disabled", true).trigger("chosen:updated");
        $("#DischargeType").attr("disabled", true).trigger("chosen:updated");

        $("#ddlDistrict").attr("disabled", true).trigger("chosen:updated");
        $("#ddlUpazila").attr("disabled", true).trigger("chosen:updated");
        $("#ddlUnion").attr("disabled", true).trigger("chosen:updated");
        $("#txtNoOfPatient").attr("disabled", true).trigger("chosen:updated");
        //$("#ddlCategory").attr("disabled", true).trigger("chosen:updated");

        $("#ReportTypeID").change(function () {

            var reporttypeid = $('#ReportTypeID').val();
            
            $("#ddlDistrict").attr("disabled", true).trigger("chosen:updated");
            $("#ddlUpazila").attr("disabled", true).trigger("chosen:updated");
            $("#ddlUnion").attr("disabled", true).trigger("chosen:updated");
            $("#txtNoOfPatient").attr("disabled", true).trigger("chosen:updated");
            $("#txtAdmissionID").attr("disabled", true).trigger("chosen:updated");

            //OP Token Details Report
            if (reporttypeid == 2) {
                $("#txtFromDate").attr("disabled", false);
                $("#txtToDate").attr("disabled", false);

                $("#ddlOPServiceType")[0].selectedIndex = 0;
                $("#ddlOPServiceType").attr("disabled", false).trigger("chosen:updated");

                $("#TokenType")[0].selectedIndex = 0;
                $("#TokenType").attr("disabled", false).trigger("chosen:updated");

                $("#ddlDept")[0].selectedIndex = 0;
                $("#ddlDept").attr("disabled", false).trigger("chosen:updated");

                $("#ddlDoc").empty();
                $("#ddlDoc").append('<option value="0">Select Doctor<option>');
                $("#ddlDoc").attr("disabled", false).trigger("chosen:updated");

                $("#ddlProject")[0].selectedIndex = 0;
                $("#ddlProject").attr("disabled", false).trigger("chosen:updated");

                $("#ddlUser")[0].selectedIndex = 0;
                $("#ddlUser").attr("disabled", false).trigger("chosen:updated");

                document.getElementById('chkStatus').checked = false;
                $("#chkStatus").attr("disabled", false);

                $("#ddlDiagnosis").empty();
                $("#ddlDiagnosis").append('<option value="0">Select Diagnosis<option>');
                $("#ddlDiagnosis").attr("disabled", true).trigger("chosen:updated");

                $("#AgeFrom").val('');
                $("#AgeFrom").attr("disabled", true);

                $("#AgeTo").val('');
                $("#AgeTo").attr("disabled", true);

                $("#SummaryType")[0].selectedIndex = 0;
                $("#SummaryType").attr("disabled", true).trigger("chosen:updated");

                $("#SummaryType")[0].selectedIndex = 0;
                $("#SummaryType").attr("disabled", true).trigger("chosen:updated");

                $("#ddlPT")[0].selectedIndex = 0;
                $("#ddlPT").attr("disabled", true).trigger("chosen:updated");

                $("#PatientType")[0].selectedIndex = 0;
                $("#PatientType").attr("disabled", true).trigger("chosen:updated");

                $("#PatientID").val('');
                $("#PatientID").attr("disabled", true);

                $("#ddlServiceType")[0].selectedIndex = 0;
                $("#ddlServiceType").attr("disabled", true).trigger("chosen:updated");

                $("#ddlService")[0].selectedIndex = 0;
